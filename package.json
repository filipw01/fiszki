{
  "name": "fiszki",
  "type": "module",
  "scripts": {
    "build": "npm run build:css && solid-start build",
    "build:css": "tailwindcss -m -i ./styles/app.css -o src/styles/app.css",
    "deploy": "fly deploy --remote-only --env PORT='8080'",
    "dev": "concurrently \"npm run dev:css\" \"solid-start dev\"",
    "dev:css": "tailwindcss -w -i ./styles/app.css -o src/styles/app.css",
    "test": "vitest",
    "e2e": "playwright test",
    "start": "solid-start start"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.245.0",
    "@motionone/solid": "^10.15.5",
    "@prisma/client": "^4.8.1",
    "@solidjs/meta": "^0.28.0",
    "@solidjs/router": "^0.6.0",
    "bcryptjs": "^2.4.3",
    "dotenv": "16.0.3",
    "js-confetti": "^0.11.0",
    "lodash-es": "^4.17.21",
    "nanoid": "^4.0.0",
    "solid-js": "^1.6.9",
    "solid-start": "^0.2.12",
    "solid-start-node": "^0.2.12",
    "undici": "^5.15.0"
  },
  "devDependencies": {
    "@playwright/test": "^1.29.2",
    "@types/bcryptjs": "^2.4.2",
    "@types/lodash-es": "^4.17.6",
    "@vitest/coverage-c8": "^0.27.1",
    "autoprefixer": "^10.4.13",
    "concurrently": "^7.6.0",
    "esbuild": "^0.16.17",
    "esbuild-register": "^3.4.2",
    "eslint": "^8.31.0",
    "postcss": "^8.4.21",
    "prettier": "^2.8.2",
    "prisma": "^4.8.1",
    "tailwindcss": "^3.2.4",
    "typescript": "^4.9.4",
    "vite": "^3.2.5",
    "vitest": "^0.27.1"
  },
  "engines": {
    "node": ">=16"
  },
  "prisma": {
    "seed": "node --require esbuild-register prisma/seed.ts"
  }
}
