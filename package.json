{
  "name": "fiszki",
  "type": "module",
  "scripts": {
    "build": "npm run build:css && solid-start build",
    "build:css": "tailwindcss -m -i ./styles/app.css -o src/styles/app.css",
    "deploy": "fly deploy --remote-only --env PORT='8080'",
    "dev": "concurrently \"npm run dev:css\" \"solid-start dev\"",
    "dev:css": "tailwindcss -w -i ./styles/app.css -o src/styles/app.css",
    "test": "vitest",
    "e2e": "playwright test",
    "start": "solid-start start"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.304.0",
    "@iconify/json": "^2.2.43",
    "@motionone/solid": "^10.16.0",
    "@prisma/client": "^4.12.0",
    "@solidjs/meta": "^0.28.2",
    "@solidjs/router": "^0.8.2",
    "bcryptjs": "^2.4.3",
    "dotenv": "16.0.3",
    "js-confetti": "^0.11.0",
    "lodash-es": "^4.17.21",
    "nanoid": "^4.0.2",
    "solid-js": "^1.7.0",
    "solid-start": "^0.2.24",
    "solid-start-node": "^0.2.24",
    "undici": "^5.21.0",
    "unplugin-icons": "^0.16.1"
  },
  "devDependencies": {
    "@faker-js/faker": "^7.6.0",
    "@playwright/test": "^1.32.1",
    "@types/bcryptjs": "^2.4.2",
    "@types/lodash-es": "^4.17.7",
    "@vitest/coverage-c8": "^0.29.8",
    "autoprefixer": "^10.4.14",
    "concurrently": "^8.0.1",
    "esbuild": "^0.17.14",
    "esbuild-register": "^3.4.2",
    "eslint": "^8.37.0",
    "postcss": "^8.4.21",
    "prettier": "^2.8.7",
    "prisma": "^4.12.0",
    "tailwindcss": "^3.3.1",
    "typescript": "^5.0.3",
    "vite": "^4.2.1",
    "vitest": "^0.29.8"
  },
  "engines": {
    "node": ">=16"
  },
  "prisma": {
    "seed": "node --loader esbuild-register/loader --require esbuild-register prisma/seed.ts"
  }
}
