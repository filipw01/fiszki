{
  "name": "fiszki",
  "type": "module",
  "scripts": {
    "build": "npm run build:css && solid-start build",
    "build:css": "tailwindcss -m -i ./styles/app.css -o src/styles/app.css",
    "deploy": "fly deploy --remote-only --env PORT='8080'",
    "dev": "concurrently \"npm run dev:css\" \"solid-start dev\"",
    "dev:css": "tailwindcss -w -i ./styles/app.css -o src/styles/app.css",
    "test": "vitest",
    "start": "solid-start start"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.229.0",
    "@prisma/client": "^4.7.1",
    "@solidjs/meta": "^0.28.0",
    "@solidjs/router": "^0.5.0",
    "bcryptjs": "^2.4.3",
    "dotenv": "16.0.3",
    "lodash-es": "^4.17.21",
    "nanoid": "^3.3.4",
    "solid-js": "^1.6.5",
    "solid-start": "^0.2.7",
    "solid-start-node": "^0.2.7",
    "undici": "^5.14.0"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.2",
    "@types/lodash-es": "^4.17.6",
    "@vitest/coverage-c8": "^0.25.8",
    "autoprefixer": "^10.4.13",
    "concurrently": "^7.6.0",
    "esbuild": "^0.16.5",
    "esbuild-register": "^3.4.1",
    "eslint": "^8.29.0",
    "postcss": "^8.4.20",
    "prettier": "^2.8.1",
    "prisma": "^4.7.1",
    "tailwindcss": "^3.2.4",
    "typescript": "^4.9.4",
    "vite": "^3.1.8",
    "vitest": "^0.25.8"
  },
  "engines": {
    "node": ">=16"
  },
  "prisma": {
    "seed": "node --require esbuild-register prisma/seed.ts"
  }
}
