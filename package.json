{
  "name": "fiszki",
  "type": "module",
  "scripts": {
    "build": "npm run build:css && vinxi build",
    "build:css": "tailwindcss -m -i ./styles/app.css -o src/styles/app.css",
    "deploy": "fly deploy --remote-only --env PORT='3000'",
    "dev": "concurrently \"npm run dev:css\" \"vinxi dev\"",
    "dev:css": "tailwindcss -w -i ./styles/app.css -o src/styles/app.css",
    "test": "vitest",
    "e2e": "playwright test",
    "local:start": "export $(grep -v '^#' .env | xargs) && npm run start",
    "start": "vinxi start"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.623.0",
    "@iconify/json": "^2.2.233",
    "@motionone/solid": "^10.16.4",
    "@prisma/client": "^4.15.0",
    "@solidjs/meta": "^0.29.4",
    "@solidjs/router": "^0.14.1",
    "@solidjs/start": "^1.0.6",
    "bcryptjs": "^2.4.3",
    "dotenv": "^16.1.4",
    "js-confetti": "^0.12.0",
    "lodash-es": "^4.17.21",
    "nanoid": "^4.0.2",
    "solid-devtools": "^0.30.1",
    "solid-js": "^1.8.19",
    "undici": "^5.22.1",
    "unplugin-icons": "^0.19.1",
    "vinxi": "^0.4.1",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@faker-js/faker": "^8.0.2",
    "@playwright/test": "^1.45.3",
    "@types/bcryptjs": "^2.4.2",
    "@types/lodash-es": "^4.17.7",
    "@vitest/coverage-c8": "^0.33.0",
    "autoprefixer": "^10.4.20",
    "concurrently": "^8.1.0",
    "esbuild": "^0.23.0",
    "esbuild-register": "^3.6.0",
    "eslint": "^8.42.0",
    "postcss": "^8.4.40",
    "prettier": "^3.3.3",
    "prisma": "^4.15.0",
    "tailwindcss": "^3.4.7",
    "typescript": "^5.5.4",
    "vitest": "^2.0.5"
  },
  "engines": {
    "node": ">=16"
  },
  "prisma": {
    "seed": "node --loader esbuild-register/loader --require esbuild-register prisma/seed.ts"
  }
}
