{
  "name": "fiszki",
  "type": "module",
  "scripts": {
    "build": "npm run build:css && vinxi build",
    "build:css": "tailwindcss -m -i ./styles/app.css -o src/styles/app.css",
    "deploy": "fly deploy --remote-only --env PORT='8080'",
    "dev": "concurrently \"npm run dev:css\" \"vinxi dev\"",
    "dev:css": "tailwindcss -w -i ./styles/app.css -o src/styles/app.css",
    "test": "vitest",
    "e2e": "playwright test",
    "start": "[ -f .env ] && export $(grep -v '^#' .env | xargs) && vinxi start || vinxi start"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.345.0",
    "@iconify/json": "^2.2.187",
    "@motionone/solid": "^10.16.2",
    "@prisma/client": "^4.15.0",
    "@solidjs/meta": "^0.29.2",
    "@solidjs/router": "^0.13.0",
    "@solidjs/start": "^0.7.3",
    "bcryptjs": "^2.4.3",
    "dotenv": "^16.1.4",
    "js-confetti": "^0.11.0",
    "lodash-es": "^4.17.21",
    "nanoid": "^4.0.2",
    "solid-devtools": "^0.29.3",
    "solid-js": "^1.8.15",
    "undici": "^5.22.1",
    "unplugin-icons": "^0.18.5",
    "vinxi": "^0.3.10",
    "zod": "^3.21.4"
  },
  "devDependencies": {
    "@faker-js/faker": "^8.0.2",
    "@playwright/test": "^1.34.3",
    "@types/bcryptjs": "^2.4.2",
    "@types/lodash-es": "^4.17.7",
    "@vitest/coverage-c8": "^0.31.4",
    "autoprefixer": "^10.4.14",
    "concurrently": "^8.1.0",
    "esbuild": "^0.20.1",
    "esbuild-register": "^3.5.0",
    "eslint": "^8.42.0",
    "postcss": "^8.4.24",
    "prettier": "^2.8.8",
    "prisma": "^4.15.0",
    "tailwindcss": "^3.3.2",
    "typescript": "^5.1.3",
    "vitest": "^0.31.4"
  },
  "engines": {
    "node": ">=16"
  },
  "prisma": {
    "seed": "node --loader esbuild-register/loader --require esbuild-register prisma/seed.ts"
  }
}
