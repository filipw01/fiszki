{
  "name": "fiszki",
  "type": "module",
  "scripts": {
    "build": "npm run build:css && solid-start build",
    "build:css": "tailwindcss -m -i ./styles/app.css -o src/styles/app.css",
    "deploy": "fly deploy --remote-only --env PORT='8080'",
    "dev": "concurrently \"npm run dev:css\" \"solid-start dev\"",
    "dev:css": "tailwindcss -w -i ./styles/app.css -o src/styles/app.css",
    "test": "vitest",
    "e2e": "playwright test",
    "start": "solid-start start"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.345.0",
    "@iconify/json": "^2.2.74",
    "@motionone/solid": "^10.16.2",
    "@prisma/client": "^4.15.0",
    "@solidjs/meta": "^0.28.5",
    "@solidjs/router": "^0.8.2",
    "bcryptjs": "^2.4.3",
    "dotenv": "^16.1.4",
    "js-confetti": "^0.11.0",
    "lodash-es": "^4.17.21",
    "nanoid": "^4.0.2",
    "solid-js": "^1.7.6",
    "solid-start": "^0.2.26",
    "solid-start-node": "^0.2.26",
    "undici": "^5.22.1",
    "unplugin-icons": "^0.16.2",
    "zod": "^3.21.4"
  },
  "devDependencies": {
    "@faker-js/faker": "^8.0.2",
    "@playwright/test": "^1.34.3",
    "@types/bcryptjs": "^2.4.2",
    "@types/lodash-es": "^4.17.7",
    "@vitest/coverage-c8": "^0.31.4",
    "autoprefixer": "^10.4.14",
    "concurrently": "^8.1.0",
    "esbuild": "^0.17.19",
    "esbuild-register": "^3.4.2",
    "eslint": "^8.42.0",
    "postcss": "^8.4.24",
    "prettier": "^2.8.8",
    "prisma": "^4.15.0",
    "solid-devtools": "^0.27.1",
    "tailwindcss": "^3.3.2",
    "typescript": "^5.1.3",
    "vite": "^4.3.9",
    "vitest": "^0.31.4"
  },
  "engines": {
    "node": ">=16"
  },
  "prisma": {
    "seed": "node --loader esbuild-register/loader --require esbuild-register prisma/seed.ts"
  }
}
